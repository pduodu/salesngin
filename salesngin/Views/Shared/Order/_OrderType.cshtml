@model string


@{
    if (@Model == OrderType.Wholesale)
    {
        <span class="badge badge-light fw-bold fs-6 px-4 py-3">
            <i class="ki-duotone ki-lots-shopping fs-2x mx-2 text-primary">
                <span class="path1"></span>
                <span class="path2"></span>
                <span class="path3"></span>
                <span class="path4"></span>
                <span class="path5"></span>
                <span class="path6"></span>
                <span class="path7"></span>
                <span class="path8"></span>
            </i>
            @OrderType.Wholesale
        </span>

    }
    else if (@Model == OrderType.Retail)
    {
        <span class="badge badge-light fw-bold fs-6 px-4 py-3">
            <i class="ki-duotone ki-package fs-2x mx-2 text-primary">
                <span class="path1"></span>
                <span class="path2"></span>
                <span class="path3"></span>
            </i>
            @OrderType.Retail
        </span>
    }
    else
    {
        <span class="badge badge-light-primary fw-bold fs-6 px-4 py-3">None</span>
    }
}